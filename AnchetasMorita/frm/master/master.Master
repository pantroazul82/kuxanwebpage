        function configurarToday(sender, args) {
            var nombre = "#" + sender._id + "_today";
            /*  alert(nombre);                */
            var panel = $(nombre);
            var text = panel.html();
            panel.html(text.replace("Today", "Hoy"));
        }


           var qc = 0;
