               btn.value = "Procesando...";
               //este no funciona si esta dentro de un update panel
               qc = qc + 1;
               if (qc > 3) {
                   //alert('Su petición se está procesando. Por favor espere un momento.');
                   swal({title:"Su petición se está procesando. Por favor espere un momento.",type:"info",confirmButtonText:"Aceptar"});
                   return false;
               } else if (qc > 1) {
                   btn.value = "Procesando...";
                   return false;                   
               }else  {
                   return true;
               }
           }

